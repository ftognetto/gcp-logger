{
  "name": "@quantos/gcp-logger",
  "version": "1.0.7",
  "description": "Quantos Gcp logger helper",
  "author": "fabriziotognetto <info@quantos.it>",
  "main": "dist/index.js",
  "repository": "github:quantosapplications/gcp-logger",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^5.2.3",
    "express": "^4.18.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^14.14.7",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^5.3.3",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
